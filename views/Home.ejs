<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Klinik Sehat</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
        }
        th, td {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h2>List Dokter</h2>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Gender</th>
                <th>Email</th>
                <th>Specialist</th>
            </tr>
        </thead>
        <tbody>
           <% dok.forEach(el => { %>
            <tr>
                <td><%= el.name %></td>
                <td><%= el.gender %></td>
                <td><%= el.email %></td>
                <td><%= el.specialist %></td>
            </tr>
           <% }) %>
        </tbody>
    </table>
</body>
</html>